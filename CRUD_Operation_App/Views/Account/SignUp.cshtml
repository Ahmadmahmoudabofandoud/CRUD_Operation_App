@model SignUpViewModel

	@{
		ViewData["Title"] = "Sign Up";
		Layout = "~/Views/Shared/_AuthLayout.cshtml";
	}

<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="SignUp">
			<div asp-validation-summary="All"></div>
			<input class="text Hamoksha" asp-for="FName" placeholder="First Name" required="">
			<input class="text Hamoksha" asp-for="LName" placeholder="Last Name" required="">
			<input class="text Hamoksha" asp-for="UserName" placeholder="UserName" required="">
			<input class="text email" asp-for="Email"  placeholder="Email" required="">
			<input class="text" asp-for="Password" placeholder="Password" required="">
			<input class="text w3lpass" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
			<div class="wthree-text">
				<label class="anim">
					<input asp-for="IsAgree"  class="checkbox" required="">
					<span>I Agree To The Terms & Conditions</span>
				</label>
				<div class="clear"> </div>
			</div>
			<input type="submit" value="SIGNUP">
		</form>
		<p>Don't You have an Account? <a asp-action="SignIn"> Login Now!</a></p>
	</div>
</div>


@section validationScriptSection {
	<partial name="_ValidationScriptsPartial" />
}